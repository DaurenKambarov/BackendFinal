<!-- views/youtube/index.ejs -->
<h1>YouTube Videos</h1>
<ul>
  <% videos.forEach(video => { %>
    <li>
      <h3><%= video.title %></h3>
      <iframe
        width="560"
        height="315"
        src="https://www.youtube.com/embed/<%= video.videoId %>"
        frameborder="0"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
        allowfullscreen
      ></iframe>
    </li>
  <% }); %>
</ul>